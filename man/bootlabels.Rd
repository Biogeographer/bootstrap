\name{bootlabels.hclust}
\alias{bootlabels.hclust}
\title{Print bootstrap values.}
\usage{
  bootlabels.hclust(x, bootstrapValues, horiz = FALSE, ...)
}
\arguments{
  \item{x}{\code{hclust} object}

  \item{bootstrapValues}{\code{numeric}, bootstrap values}

  \item{horiz}{print values for a horizontal tree?}
}
\description{
  This function prints bootstrap values to the
  corresponding node.
}
\seealso{
  \code{\link{bootstrap}}
}

